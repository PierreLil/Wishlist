<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView5Name"
        android:layout_width="142dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="76dp"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Name"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3Name"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="52dp"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView4Name"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4Name"
        android:layout_width="142dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="56dp"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView5Name"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView1Name"
        android:layout_width="139dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="56dp"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView2Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.86" />

    <Button
        android:id="@+id/button"
        android:layout_width="112dp"
        android:layout_height="54dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/Url"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:hint="Ex: http://example.com"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:autofillHints="" />

    <EditText
        android:id="@+id/Name"
        android:layout_width="128dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:hint="Item Name"
        app:layout_constraintBottom_toTopOf="@+id/Url"
        app:layout_constraintStart_toStartOf="parent"
        android:autofillHints="" />

    <EditText
        android:id="@+id/Price"
        android:layout_width="104dp"
        android:layout_height="45dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="number|numberDecimal"
        android:hint="Ex: 5.99"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        android:autofillHints="" />

    <TextView
        android:id="@+id/textView2Name"
        android:layout_width="141dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="52dp"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView3Name"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2Price"
        android:layout_width="72dp"
        android:layout_height="41dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="4dp"
        android:gravity="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView2Url"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView5Url"
        android:layout_width="117dp"
        android:layout_height="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="88dp"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/Price"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView4Url"
        android:layout_width="115dp"
        android:layout_height="18dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="28dp"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/textView5Price"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView4Price"
        android:layout_width="72dp"
        android:layout_height="41dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="4dp"
        android:gravity="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView4Url"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView3Price"
        android:layout_width="72dp"
        android:layout_height="41dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="4dp"
        android:gravity="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView3Url"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView1Price"
        android:layout_width="72dp"
        android:layout_height="41dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="4dp"
        android:gravity="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView1Url"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView5Price"
        android:layout_width="72dp"
        android:layout_height="41dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="4dp"
        android:gravity="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView5Url"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView3Url"
        android:layout_width="121dp"
        android:layout_height="20dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="40dp"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/textView4Price"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView1Url"
        android:layout_width="126dp"
        android:layout_height="19dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="36dp"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/textView2Price"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView2Url"
        android:layout_width="121dp"
        android:layout_height="19dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="24dp"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/textView3Price"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>